<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav_graph"
    app:startDestination="@id/mapsFragment">

    <fragment
        android:id="@+id/mapsFragment"
        android:name="com.kcc.kmmhackathon.androidHackathonApp.MapsFragment"
        android:label="fragment_maps"
        tools:layout="@layout/fragment_maps" />
    <action
        android:id="@+id/action_studiesListFragment_to_aboutFragment"
        app:destination="@id/aboutFragment"
        app:enterAnim="@animator/animator_default_screen_enter"
        app:exitAnim="@animator/animator_default_screen_exit"
        app:popEnterAnim="@android:animator/fade_in"
        app:popExitAnim="@animator/animator_default_screen_exit" />
    <action
        android:id="@+id/action_studiesListFragment_to_studyDetailsFragment"
        app:destination="@id/studyDetailsFragment" />

    <activity
        android:id="@+id/mainActivity"
        android:name="com.kcc.kmmhackathon.androidHackathonApp.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <activity
        android:id="@+id/addMealActivity"
        android:name="com.kcc.kmmhackathon.androidHackathonApp.AddMealActivity"
        android:label="add_meal_activity"
        tools:layout="@layout/add_meal_activity" />
    <activity
        android:id="@+id/findMealActivity"
        android:name="com.kinandcarta.lib.find.meal.view.FindMealActivity"
        android:label="find_meal_activity"
        tools:layout="@layout/find_meal_activity" />

</navigation>